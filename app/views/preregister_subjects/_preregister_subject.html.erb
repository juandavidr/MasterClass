    <%= preregister_subject.semester.name %> <%= preregister_subject.subject.name %> <%= preregister_subject.status %>
    <% if (current_user?(@user) || coordinador?) && preregister_subject.semester.startdate > Time.now %>
      <%= link_to "Editar", edit_preregister_subject_path(preregister_subject,:program_id =>@program.id,:user_id =>@user.id) %> | <%= link_to "Eliminar", preregister_subject, method: :delete,
        data: { confirm: "Â¿Esta seguro de realizar esta accion?" } %>
    <% end %>
    <% if (current_user?(@user) || coordinador?) && preregister_subject.semester.startdate < Time.now && preregister_subject.semester.finaldate > Time.now %>
